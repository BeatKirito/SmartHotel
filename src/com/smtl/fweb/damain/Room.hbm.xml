<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.smtl.fweb.damain">
	<class	name="Room" table="tb_room">
		<id	name="roomID" type="integer" column="roomID">
			<generator class="identity"/>
		</id>
		
		<property name="roomTypeID" column="roomTypeID"
			type="integer" />
		<property name="roomNum" column="roomNum"
			type="string" length="10" />
		<property name="floor" column="floor"
			type="integer" />
		<property name="state" column="state"
			type="boolean" />
		<property name="password" column="password"
			type="string" length="50" /> 
	</class>
</hibernate-mapping>